function rpsls(pl1,pl2){
if(pl1 === 'rock' && pl2 === 'lizard' || pl1 === 'paper' && pl2 === 'rock' || pl1 === 'scissors' && pl2 === 'lizard' || pl1 === 'lizard' && pl2 === 'paper' || pl1 === 'spock' && pl2 === 'rock' ){
  return "Player 1 Won!"
} else if (pl2 === 'scissors' && pl1 === 'lizard' || pl2 === 'lizard' && pl1 === 'spock' || pl2 === 'lizard' && pl1 === 'paper' ||  pl2 === 'spock' && pl1 === 'scissors' || pl2 === 'spock' && pl1 === 'rock'){
  return "Player 2 Won!"
} else {
  return "Draw!"
}
}
